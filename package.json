{
  "name": "fs-json-store",
  "version": "2.3.0",
  "description": "Node.js module for storing JSON data on the file system",
  "author": "Vladimir Yakovlev <dev.vladimir.y@gmail.com> (https://github.com/vladimiry)",
  "license": "MIT",
  "repository": "git@github.com:vladimiry/fs-json-store.git",
  "keywords": [
    "atomic",
    "consistency",
    "data",
    "fs",
    "json",
    "persistence",
    "storage",
    "store"
  ],
  "engines": {
    "node": ">=8.9.4 <9 || >=10.0.0"
  },
  "main": "./index.js",
  "scripts": {
    "dist:compile": "tsc -p ./src/lib/tsconfig.json --listEmittedFiles",
    "dist:compile:watch": "tsc -p ./src/lib/tsconfig.json -w",
    "dist:clean": "rimraf ./dist",
    "dist:copy": "cpx \"{package.json,README.md,LICENSE}\" ./dist",
    "dist": "npm-run-all dist:clean lint dist:compile dist:copy test",
    "lint:lib": "tslint -p ./src/lib/tsconfig.json -c ./tslint.json \"./src/{lib,@types}/**/*.ts\"",
    "lint:test": "tslint -p ./src/test/tsconfig.json -c ./src/test/tslint.json \"./src/test/**/*.ts\"",
    "lint": "npm-run-all lint:lib lint:test",
    "test": "cross-env TS_NODE_PROJECT=./src/test/tsconfig.json ava --verbose \"./src/test/**/*.spec.ts\""
  },
  "pre-commit": [
    "lint"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "devDependencies": {
    "@types/node": "^10.12.24",
    "@types/proper-lockfile": "^3.0.0",
    "@types/randomstring": "^1.1.6",
    "@types/sinon": "^7.0.5",
    "ava": "^1.2.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "randomstring": "^1.1.5",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "tslint-rules-bunch": "^0.0.7",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "combine-errors": "^3.0.3",
    "fs-no-eperm-anymore": "^2.0.5",
    "imurmurhash": "^0.1.4",
    "kind-of": "^6.0.2",
    "memfs": "^2.15.1",
    "proper-lockfile": "^3.2.0",
    "tslib": "^1.9.3"
  }
}
